{
  "name": "@ston-fi/omniston-sdk",
  "version": "0.7.8",
  "description": "Typescript SDK for the STON.fi Omniston protocol",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ston-fi/omniston-sdk.git",
    "directory": "packages/omniston-sdk"
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "tsdown --sourcemap --watch ./src",
    "build": "tsdown",
    "format": "pnpm run /^format:.*/",
    "format:biome": "biome format --write",
    "lint": "pnpm run /^lint:.*/",
    "lint:tsc": "tsc --noEmit --pretty",
    "lint:biome": "biome check",
    "test": "vitest",
    "proto:generate": "node scripts/generate-proto.js",
    "prepublishOnly": "pnpm run lint && pnpm run test --run && pnpm run build && pnpm attw --pack . && pnpm publint"
  },
  "dependencies": {
    "isomorphic-ws": "5.0.0",
    "json-rpc-2.0": "1.7.0",
    "rxjs": "7.8.1",
    "type-fest": "5.0.1",
    "ws": "8.17.1"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "catalog:",
    "@protobuf-ts/protoc": "2.11.1",
    "@types/node": "24.0.8",
    "@types/ws": "8.5.10",
    "publint": "catalog:",
    "ts-proto": "2.8.0",
    "tsdown": "catalog:",
    "vitest": "catalog:"
  }
}
